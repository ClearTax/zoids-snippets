{
  "importZoids": {
    "description": "Import Zoids Components",
    "prefix": "zi",
    "body": [
      "import { ${1: module} } from '@cleartax/zoids';"
    ]
  },
  "zoidsContianer": {
    "description": "Zoids Container",
    "prefix": "zcon",
    "body": [
      "<Container>",
      "\t$0",
      "</Container>"
    ]
  },
  "zoidsCell": {
    "description": "Zoids Cell",
    "prefix": "zcel",
    "body": [
      "<Cell width='$1'>",
      "\t$0",
      "</Cell>"
    ]
  },
  "zoidsCellClassName": {
    "description": "Zoids Cell with className",
    "prefix": "zcelc",
    "body": [
      "<Cell width='$1' className='$2'>",
      "\t$0",
      "</Cell>"
    ]
  },
  "zoidsSkeleton": {
    "description": "Zoids Skeleton",
      "prefix": "zsk",
      "body": [
        "<Skeleton active$1 title$2 loading={$3} paragraph={$4}>",
        "\t$0",
        "</Skeleton>"
    ]
  },
  "importZdButton": {
    "description": "Import Zoids Button",
    "prefix": "zib",
    "body": [
      "import { Button } from '@cleartax/zoids';"
    ]
  },
  "zoidsButton": {
    "description": "Zoids Button",
    "prefix": "zbu",
    "body": [
      "<Button nature='$1' size='$2' onClick={$3}>",
      "\t$0",
      "</Button>"
    ]
  },
  "zoidsCard": {
    "description": "Zoids card",
    "prefix": "zcard",
    "body": [
      "<Card nature='$1'>",
      "\t$0",
      "</Card>"
    ]
  },
  "zoidsCardWithProps": {
    "description": "Zoids card with props",
    "prefix": "zcardp",
    "body": [
      "<Card id='$1' nature='$2' align='left$3' compact$4 border$5 hoverable$6>",
      "\t$0",
      "</Card>"
    ]
  }
}
