{
  "importZoids": {
    "description": "Import Zoids Components",
    "prefix": "zi",
    "body": [
      "import { ${1: module} } from '@cleartax/zoids';"
    ]
  },
  "zoidsContianer": {
    "description": "Zoids Container",
    "prefix": "zcon",
    "body": [
      "<Container>",
      "\t$0",
      "</Container>"
    ]
  },
  "zoidsCell": {
    "description": "Zoids Cell",
    "prefix": "zcel",
    "body": [
      "<Cell width='$1'>",
      "\t$0",
      "</Cell>"
    ]
  },
  "zoidsCellClassName": {
    "description": "Zoids Cell with className",
    "prefix": "zcelc",
    "body": [
      "<Cell width='$1' className='$2'>",
      "\t$0",
      "</Cell>"
    ]
  },
  "zoidsSkeleton": {
    "description": "Zoids Skeleton",
      "prefix": "zsk",
      "body": [
        "<Skeleton active$1 title$2 loading={$3} paragraph={$4}>",
        "\t$0",
        "</Skeleton>"
    ]
  },
  "zoidsHeading": {
    "description": "Zoids Heading",
      "prefix": "zhe",
      "body": [
        "<Heading text='$1' size='$2'/>"
    ]
  },
  "zoidsText": {
    "description": "Zoids Text",
      "prefix": "zte",
      "body": [
